    "jest": "29.5.0",
    "jest-canvas-mock": "2.5.0",
    "jest-environment-jsdom": "29.5.0",
    "prettier": "^2.8.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "ts-jest": "29.1.0",
    "typescript": "5.0.4"
  },
  "dependencies": {
    "@mui/material": "^5.12.2"
