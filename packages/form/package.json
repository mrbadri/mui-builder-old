    "setupFilesAfterEnv": [
      "./jest.setup.ts"
    ]
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "14.0.0",
